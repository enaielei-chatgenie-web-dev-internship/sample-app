  "license": "MIT",
  "scripts": {
    "ch-main": "git checkout main",
    "ch-prev": "git checkout @{-1}",
    "ad": "git add --all",
    "co": "git commit -aqm \"'Commit changes\"",
    "pu": "git push -u origin ./",
    "pu-hero": "git push heroku",
    "me": "git merge @{-1}",
    "he-drop": "heroku pg:reset DATABASE --confirm enaielei-chatgenie-sample-app",
    "he-open": "heroku open",
    "he-migrate": "heroku run rails db:migrate",
    "he-seed": "heroku run rails db:seed",
    "he-reset": "yarn run he-drop && yarn run he-migrate && yarn run he-seed",
    "dp": "yarn run ad && yarn run co && yarn run pu && yarn run ch-main && yarn run me && yarn run pu && yarn run pu-hero && yarn run he-reset && yarn run he-open"
  }
}
